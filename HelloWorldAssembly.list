F00:0175       ;;;;;;;;;;;;;;;;;;; vdp_initialize_pattern_table ;;;;;;;;;;;;;;;;;;;
F00:0176       
F00:0177       vdp_initialize_pattern_table:
F00:0178         pha
               S01:0000C0FA:  48
F00:0179         phx
               S01:0000C0FB:  DA
F00:0180       
F00:0181         lda #<(VDP_PATTERN_TABLE_BASE)
               S01:0000C0FC:  A9 00
F00:0182         sta VDP_REG
               S01:0000C0FE:  8D 01 B0
F00:0183       
F00:0184         lda #(VDP_WRITE_VRAM_BIT | >VDP_PATTERN_TABLE_BASE)
               S01:0000C101:  A9 48
F00:0185         sta VDP_REG
               S01:0000C103:  8D 01 B0
F00:0186       
F00:0187       
F00:0188         lda #<vdp_patterns                      ; load the start address of the patterns to zero page
               S01:0000C106:  A9 00
F00:0189         sta VDP_PATTERN_INIT
               S01:0000C108:  85 30
F00:0190         lda #>vdp_patterns
               S01:0000C10A:  A9 C2
F00:0191         sta VDP_PATTERN_INIT_HI
               S01:0000C10C:  85 31
F00:0192       vdp_pattern_table_loop:
F00:0193         lda (VDP_PATTERN_INIT)                  ; load A with the value at VDP_PATTERN_INIT
               S01:0000C10E:  B2 30
F00:0194         sta VDP_VRAM                            ; and store it to VRAM
               S01:0000C110:  8D 00 B0
F00:0195         lda VDP_PATTERN_INIT                    ; load the low byte of VDP_PATTERN_INIT address into A
               S01:0000C113:  A5 30
F00:0196         clc                                     ; clear carry flag
               S01:0000C115:  18
F00:0197         adc #1                                  ; Add 1, with carry
               S01:0000C116:  69 01
F00:0198         sta VDP_PATTERN_INIT                    ; store back into VDP_PATTERN_INIT
               S01:0000C118:  85 30
F00:0199         lda #0                                  ; load A with 0
               S01:0000C11A:  A9 00
F00:0200         adc VDP_PATTERN_INIT_HI                 ; add with the carry flag to the high address
               S01:0000C11C:  65 31
F00:0201         sta VDP_PATTERN_INIT_HI                 ; and store that back into the high byte
               S01:0000C11E:  85 31
F00:0202         cmp #>vdp_end_patterns                  ; compare if we're at the end of the patterns
               S01:0000C120:  C9 C6
F00:0203         bne vdp_pattern_table_loop              ; if not, loop again
               S01:0000C122:  D0 EA
F00:0204         lda VDP_PATTERN_INIT                    ; compare the low byte
               S01:0000C124:  A5 30
F00:0205         cmp #<vdp_end_patterns
               S01:0000C126:  C9 00
F00:0206         bne vdp_pattern_table_loop              ; if not equal, loop again
               S01:0000C128:  D0 E4
F00:0207         plx
               S01:0000C12A:  FA
F00:0208         pla
               S01:0000C12B:  68
F00:0209         rts
               S01:0000C12C:  60
