F00:0047       ;;;;;;;;;;;;;;;;;;; vdp_set_registers ;;;;;;;;;;;;;;;;;;;;;;;;
F00:0048       
F00:0049       vdp_set_registers:
F00:0050         pha
               S01:0000C016:  48
F00:0051         phx
               S01:0000C017:  DA
F00:0052         ldx #0
               S01:0000C018:  A2 00
F00:0053       .loop:
F00:0054         lda vdp_register_inits,x
               S01:0000C01A:  BD 2E C0
F00:0055         sta VDP_REG
               S01:0000C01D:  8D 01 B0
F00:0056         txa
               S01:0000C020:  8A
F00:0057         ora #VDP_REGISTER_BITS 				; combine the register number with the second write pattern
               S01:0000C021:  09 80
F00:0058         sta VDP_REG
               S01:0000C023:  8D 01 B0
F00:0059         inx
               S01:0000C026:  E8
F00:0060         cpx #(vdp_end_register_inits - vdp_register_inits) 	; compute number of registers
               S01:0000C027:  E0 08
F00:0061         bne .loop
               S01:0000C029:  D0 EF
F00:0062         plx
               S01:0000C02B:  FA
F00:0063         pla
               S01:0000C02C:  68
F00:0064         rts
               S01:0000C02D:  60
F00:0065       
F00:0066       vdp_register_inits:
F00:0067       vdp_register_0: .byte %00000000 ; 0  0  0  0  0  0  M3 EXTVDP
               S01:0000C02E:  00
F00:0068       vdp_register_1: .byte %10010000 ;16k Bl IE M1 M2 0 Siz MAG
               S01:0000C02F:  90
F00:0069       vdp_register_2: .byte $00       ; Name table base / $400. $00 = $0000
               S01:0000C030:  00
F00:0070       vdp_register_3: .byte $00       ; Color table base (currently unused)
               S01:0000C031:  00
F00:0071       vdp_register_4: .byte $01       ; Pattern table base / $800. $01 = $0800
               S01:0000C032:  01
F00:0072       vdp_register_5: .byte $00       ; Sprite attribute table base (currently unused)
               S01:0000C033:  00
F00:0073       vdp_register_6: .byte $00       ; Sprite pattern generator (currently unused)
               S01:0000C034:  00
F00:0074       vdp_register_7: .byte $F1       ; FG/BG. F=>White, 1=>Black
               S01:0000C035:  F1
F00:0075       vdp_end_register_inits:
