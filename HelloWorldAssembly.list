F00:0011       ; TMS9918A
F00:0012       VDP_VRAM               = $B000		; address to set MODE low for a video ram operation on the TMS9918A Video Display Proces
F00:0013       VDP_REG                = $B001		; address to set MODE high for a video register operation on the TMS9918A Video Display 
F00:0014       VDP_WRITE_VRAM_BIT     = %01000000  	; pattern of second vram address write: 01AAAAAA
F00:0015       VDP_REGISTER_BITS      = %10000000  	; pattern of second register write: 10000RRR
F00:0016       
F00:0017       VDP_NAME_TABLE_BASE = $0000
F00:0018       VDP_PATTERN_TABLE_BASE = $0800
F00:0019       
F00:0020       ; zero page addresses
F00:0021       VDP_PATTERN_INIT    	= $30
F00:0022       VDP_PATTERN_INIT_HI 	= $31
F00:0023       
F00:0024         .org $C000
