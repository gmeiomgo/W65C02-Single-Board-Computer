F00:0078       ;;;;;;;;;;;;;;;;;;; clear_vram ;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
F00:0079       
F00:0080       clear_vram:
F00:0081         ldy #$40				; setup vram address
               S01:0000C036:  A0 40
F00:0082         lda #0
               S01:0000C038:  A9 00
F00:0083         sta VDP_REG
               S01:0000C03A:  8D 01 B0
F00:0084         sty VDP_REG
               S01:0000C03D:  8C 01 B0
F00:0085         ldx #$c0				; count high
               S01:0000C040:  A2 C0
F00:0086       nexf:
F00:0087         ldy #0				; count low
               S01:0000C042:  A0 00
F00:0088       fill_:
F00:0089        lda #$20
               S01:0000C044:  A9 20
F00:0090         sta VDP_VRAM				; write a zero to vram
               S01:0000C046:  8D 00 B0
F00:0091         iny					; next loc
               S01:0000C049:  C8
F00:0092         bne fill_				; keep going until page done
               S01:0000C04A:  D0 F8
F00:0093         inx					; done all 40 pages?
               S01:0000C04C:  E8
F00:0094         bne nexf				; no, keep going
               S01:0000C04D:  D0 F3
F00:0095         rts
               S01:0000C04F:  60
